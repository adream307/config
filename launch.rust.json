    "version": "0.2.0",
    "configurations": [
        {
            "name": "attach-portlist",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/target/debug/makepad-example-portlist",
            "MIMode": "gdb",
            "miDebuggerPath": "/home/cyf/.cargo/bin/rust-gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
